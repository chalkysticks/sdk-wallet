{
    "author": "ChalkySticks LLC",
    "description": "ChalkySticks SDK web-based clients for Wallet and Payments",
    "devDependencies": {
        "@babel/cli": "^7.13.16",
        "@babel/core": "^7.14.0",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
        "@babel/preset-env": "^7.14.1",
        "@babel/preset-typescript": "^7.13.0",
        "@chalkysticks/mrm-preset": "github:chalkysticks/mrm-preset",
        "@chalkysticks/sdk-core": "github:chalkysticks/sdk-core",
        "@types/expect": "^24.3.0",
        "@types/mocha": "^7.0.2",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "babel-plugin-add-import-extension": "^1.5.1",
        "dispatcher": "github:mattkenefick/dispatcher",
        "eloquent-js": "github:mattkenefick/eloquentjs",
        "eslint": "^7.25.0",
        "prettier": "^2.2.1",
        "ts-mocha": "^7.0.0",
        "tslib": "^2.2.0"
    },
    "exports": {
        ".": {
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "files": [
        "dist/*"
    ],
    "license": "ISC",
    "main": "dist/cjs/index.js",
    "module": "dist/mjs/index.js",
    "name": "@chalkysticks/sdk-wallet",
    "scripts": {
        "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json && bash ./node_modules/@chalkysticks/mrm-preset/fix-packages.sh",
        "postversion": "git push && git push --tags",
        "prettier": "prettier -c src/**/*.ts",
        "test": "ts-mocha -p ./node_modules/@chalkysticks/mrm-preset/tsconfig.test.json test/**/*.spec.ts",
        "version": "npm run format && git add -A src"
    },
    "types": "dist/mjs/index.d.ts",
    "version": "1.0.0"
}
